{
    "compilerOptions": {
        "target": "es2017",
        "allowSyntheticDefaultImports": false,
        "baseUrl": "./",
        "paths": {
            "Middlewares/*": ["src/app/middlewares/*"],
            "Errors/*": ["src/app/errors/*"],
            "Constants/*": ["src/app/constants/*"],
            "Repositories/*": ["src/app/repositories/*"]
        }
    },
    "exclude": ["node_modules", "dist", "scripts", "environments"]
}