{
  "name": "my-new-api-microservice",
  "version": "1.0.0",
  "description": "Serverless microservice api template",
  "scripts": {
    "server": "sls offline start --stage local",
    "deploy-dev": "./scripts/serverless.sh -s dev",
    "deploy-test": "./scripts/serverless.sh -s test",
    "deploy-prod": "./scripts/serverless.sh -s prod",
    "invoke-dev": "./scripts/serverless.sh -s dev -t invoke",
    "invoke-test": "./scripts/serverless.sh -s test -t invoke",
    "invoke-prod": "./scripts/serverless.sh -s prod -t invoke",
    "logs-dev": "./scripts/serverless.sh -s dev -t logs",
